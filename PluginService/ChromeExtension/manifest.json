{
  "name": "Benutzername",
  "version": "1.0",
  "description": "Traegt den Benutzernamen in das aktive Eingabefeld ein.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "http://*/",
    "https://*/",
    "http://localhost:9000/"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [ "jQuery.js", "background.js" ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/InputUsername16.png",
      "32": "images/InputUsername32.png",
      "48": "images/InputUsername48.png",
      "128": "images/InputUsername128.png"
    }
  },
  "icons": {
    "16": "images/InputUsername16.png",
    "32": "images/InputUsername32.png",
    "48": "images/InputUsername48.png",
    "128": "images/InputUsername128.png"
  },
  "manifest_version": 2
}
